<div class="contentModule">
  <div class="grid">
    <div class="left">
      <img src="{{ component.mainSrc }}" alt="{{ appImage.main.alt }}">
    </div>
    <div class="right">
      {% for image in component.sideImages %}
        <img src="{{ image.src }}" alt="{{ image.alt }}">
      {% endfor %}
    </div>
  </div>
</div>